{
    "version": "4.1.6",
    "description": "qBittorrent. Free and reliable P2P Bittorent client.",
    "homepage": "https://www.qbittorrent.org/",
    "license": {
        "identifier": "GPL-2.0-only",
        "url": "https://github.com/qbittorrent/qBittorrent/blob/master/COPYING"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-4.1.6/qbittorrent_4.1.6_x64_setup.exe#/cosi.7z",
            "hash": "32091feda94b25952cd7d22c64b7a45d2a541ab400cd5ff67ff26a601a5466da"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-4.1.6/qbittorrent_4.1.6_setup.exe#/cosi.7z",
            "hash": "28816da7ef958a3e96c5363994ab7bd60e68c375287d6989ad0b813c7d434c3c"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\" -Force -Recurse",
    "persist": "profile",
    "bin": [
        [
            "qbittorrent.exe",
            "qBittorrent",
            "--portable"
        ]
    ],
    "shortcuts": [
        [
            "qbittorrent.exe",
            "qBittorrent",
            "--portable"
        ]
    ],
    "checkver": {
        "url": "https://www.qbittorrent.org/download.php",
        "regex": "Latest:\\s+v([\\d\\.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-$version/qbittorrent_$version_x64_setup.exe#/cosi.7z",
                "hash": {
                    "url": "https://www.qbittorrent.org/download.php",
                    "regex": "64-bit.*\\s+.*<code>($sha256)</code>"
                }
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-$version/qbittorrent_$version_setup.exe#/cosi.7z",
                "hash": {
                    "url": "https://www.qbittorrent.org/download.php",
                    "regex": "32-bit.*\\s+.*<code>($sha256)</code>"
                }
            }
        }
    }
}
