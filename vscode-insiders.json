{
    "version": "1.25.0-insider",
    "license": "https://code.visualstudio.com/License",
    "description": "New type of tool that combines the simplicity of a code editor with what developers need for their core edit-build-debug cycle.",
    "homepage": "https://code.visualstudio.com/",
    "architecture": {
        "64bit": {
            "url": "https://vscode-update.azurewebsites.net/1.25.0-insider/win32-x64-archive/insider#/cosi.7z"
        },
        "32bit": {
            "url": "https://vscode-update.azurewebsites.net/1.25.0-insider/win32-archive/insider#/cosi.7z"
        }
    },
    "post_install": [
        "Move-Item $dir\\* $dir\\Code -Force",
        "New-Item code-insiders-portable-data -Type Directory",
        "New-Item code-insiders-portable-data\\tmp -Type Directory"
    ],
    "persist": [
        "code-insiders-portable-data"
    ],
    "bin": [
        "Code\\bin\\code-insiders.cmd",
        "Code\\Code - Insiders.exe"
    ],
    "shortcuts": [
        [
            "Code\\Code - Insiders.exe",
            "Visual Studio Code Insiders"
        ]
    ],
    "checkver": {
        "url": "https://code.visualstudio.com/sha?build=insider",
        "jp": "$.products[?(@.platform.os == 'win32-x64-archive')].name"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://vscode-update.azurewebsites.net/$version/win32-x64-archive/insider#/cosi.7z"
            },
            "32bit": {
                "url": "https://vscode-update.azurewebsites.net/$version/win32-archive/insider#/cosi.7z"
            }
        }
    }
}
